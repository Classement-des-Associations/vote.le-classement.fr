{
  "name": "vote.le-classement.fr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "packageManager": "pnpm@8.1.1",
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^3.0.0",
    "@symfony/webpack-encore": "^4.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "adonis-preset-ts": "^2.1.0",
    "adonis-sail": "^0.0.7",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.2.4",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5",
    "unpoly": "^2.7.2",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.0"
  },
  "dependencies": {
    "@adonisjs/attachment-lite": "^1.0.7",
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/limiter": "^1.0.1",
    "@adonisjs/lucid": "^18.4.0",
    "@adonisjs/lucid-slugify": "^2.2.1",
    "@adonisjs/mail": "^8.1.2",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/route-model-binding": "^1.0.1",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.1.0",
    "@adonisjs/validator": "^12.5.0",
    "@adonisjs/view": "^6.2.0",
    "@alpinejs/collapse": "^3.12.0",
    "@alpinejs/focus": "^3.12.0",
    "@iconify-json/heroicons": "^1.1.12",
    "adonis-lucid-filter": "^4.1.0",
    "alpinejs": "^3.12.0",
    "aws-sdk": "^2.1353.0",
    "chart.js": "^4.2.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^2.30.0",
    "disposable-email-domains": "^1.0.62",
    "edge-iconify": "^1.0.1",
    "file-loader": "^6.2.0",
    "luxon": "^3.3.0",
    "markdown-it": "^13.0.1",
    "mjml": "^4.14.0",
    "pg": "^8.10.0",
